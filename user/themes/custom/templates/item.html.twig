{% extends 'partials/base.html.twig' %}

{% block content %}
    <section class="section">
        <h1>{{ page.title }}</h1>

        {{ page.content|raw }}
    </section>
    <section class="section pt-0">
        {% for image in page.media.images %}
            <img src="{{ image.cropResize(640, 3000).url }}" alt="{{ image.name }}">
        {% endfor %}
    </section>
{% endblock %}